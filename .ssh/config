Include ~/.orbstack/ssh/config

Host github.com
  AddKeysToAgent yes
  # Omit if no passphrase
  UseKeychain yes
  IdentitiesOnly yes
  IdentityFile ~/.ssh/github
  # Hostname ssh.github.com
  # Port 443
  # User git

Host ssh.satorinet-pools.com
  User devchris
  PasswordAuthentication no
  IdentityFile ~/.ssh/id_satorinet_pools
  SetEnv TERM=xterm-256color

Host 82.66.89.60
  User wimo
  Port 38001
  SetEnv TERM=xterm-256color
